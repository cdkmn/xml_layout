<Scaffold>
    <attr:appBar>
        <AppBar>
            <attr:title>
                <Text>Grid Example</Text>
            </attr:title>
        </AppBar>
    </attr:appBar>
    <attr:body>
        <GridView.count crossAxisCount="3">
            <for array="${map.pictures}">
                <Container color="black26">
                    <MaterialButton>
                        <attr:onPressed>
                            <Function>
                                <Call function="$print">
                                    <Argument value="$index"/>
                                </Call>
                            </Function>
                        </attr:onPressed>
                        <Image.network>$item</Image.network>
                    </MaterialButton>
                </Container>
            </for>
        </GridView.count>
    </attr:body>
</Scaffold>